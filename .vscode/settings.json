{
    "C/C++ Include Guard.Macro Type": "Filepath",
    "C/C++ Include Guard.Path Depth": 1,
    "C/C++ Include Guard.Remove Extension": false,
    "C/C++ Include Guard.Prefix": "KRX_",
    "C/C++ Include Guard.Suffix": "",
	"fileHeaderComment.parameter":{
		"*":{
			"commentbegin": "/*",
			"commentprefix": " *",
			"commentend": " */",
			"company": "Your Company"
		}
	},
	"fileHeaderComment.template":{
		"*":[
			"${commentbegin}",
			"${commentprefix} Copyright (c) ${year} NetaScale Object Solutions.",
			"${commentprefix} Created on ${date}.",
			"${commentend}",
            "${commentbegin}",
            "${commentprefix} This Source Code Form is subject to the terms of the Mozilla Public",
            "${commentprefix}  License, v. 2.0. If a copy of the MPL was not distributed with this",
            "${commentprefix}  file, You can obtain one at https://mozilla.org/MPL/2.0/.",
            "${commentend}"
		]
	},
	"esbonio.sphinx.confDir": "${workspaceFolder}/docs/notes",
	"C/C++ Include Guard.Auto Update Include Guard": false,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"files.trimFinalNewlines": true,
	"editor.renderFinalNewline": "dimmed"

}
